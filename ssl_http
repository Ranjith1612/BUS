package org;


import java.io.IOException;
import java.security.SecureRandom;
import java.security.cert.CertificateException;
import java.security.cert.X509Certificate;

import javax.net.ssl.HostnameVerifier;
import javax.net.ssl.SSLContext;
import javax.net.ssl.SSLSession;
import javax.net.ssl.SSLSocketFactory;
import javax.net.ssl.TrustManager;
import javax.net.ssl.X509TrustManager;

import okhttp3.OkHttpClient;
import okhttp3.Request;
import okhttp3.Response;

public class ssl_okhttp2 {
public void ss() throws IOException
{
	OkHttpClient client = new OkHttpClient();
	
	Request request = new Request.Builder()
		.url("https://redbus.p.rapidapi.comwww.redbus.in/Mayiladuthurai?fromCityId=466&toCityId=66065&onward=28-Mar-2020&fromCityName=Mayiladuthurai&toCityName=CMBT%252C%2520Chennai")
		.get()
		.addHeader("x-rapidapi-host", "redbus.p.rapidapi.com")
		.addHeader("x-rapidapi-key", "5533bd7babmshc67016969c81137p1627cdjsn9f65e83602bb")
		.build();


Response response = client.newCall(request).execute();
System.out.println(response);
}

public static void main(String[] args) throws IOException {
	ssl_okhttp2 dd=new ssl_okhttp2();
	dd.ss();
}
}
